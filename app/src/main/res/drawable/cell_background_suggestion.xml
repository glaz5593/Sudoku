<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item >
        <shape android:shape="rectangle">
            <solid android:color="#ffffff"/>
        </shape>
    </item>

    <item android:top="2dp" android:bottom="2dp" android:left="2dp" android:right="2dp">
        <shape android:shape="rectangle">
            <corners android:radius="6dp"/>
            <gradient android:startColor="#EFBE64" android:endColor="#9C5213" android:angle="270"/>
        </shape>
    </item>

    <item android:top="4dp" android:bottom="4dp" android:left="4dp" android:right="4dp">
        <shape android:shape="rectangle">
            <corners android:radius="6dp"/>
            <gradient android:startColor="#EFBE64" android:endColor="#B67034" android:angle="270"/>
        </shape>
    </item>
</layer-list>